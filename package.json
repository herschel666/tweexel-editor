{
  "name": "tweexel-editor",
  "description": "Editor for emoji-based pixel art on twitter.",
  "version": "0.1.0-alpha.01",
  "main": "index.html",
  "keywords": [],
  "author": "Emanuel Kluge <kraemerseele@gmx.de>",
  "license": "MIT",
  "scripts": {
    "start": "preact watch",
    "build": "preact build",
    "postbuild": "cp build/404/index.html build/404.html",
    "lint": "eslint --fix src/**/*.{js,ts,tsx}",
    "test": "jest",
    "preserve": "npm run build",
    "serve": "serve -s -C -n build",
    "precommit": "lint-staged",
    "deploy": "npm-run-all lint test build"
  },
  "lint-staged": {
    "*.{css,md,scss}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintIgnore": [
    "build/*"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "clipboard-polyfill": "^2.8.6",
    "preact": "^10.3.1",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "3.2.1",
    "tailwindcss": "^1.2.0"
  },
  "devDependencies": {
    "@testing-library/preact": "1.0.2",
    "@types/classnames": "2.2.10",
    "@types/jest": "25.2.3",
    "@types/webpack-env": "1.15.2",
    "@typescript-eslint/eslint-plugin": "3.1.0",
    "@typescript-eslint/parser": "3.1.0",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "husky": "4.2.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.0.1",
    "lint-staged": "10.2.9",
    "npm-run-all": "4.1.5",
    "preact-cli": "3.0.0-rc.14",
    "prettier": "2.0.5",
    "serve": "11.3.2",
    "ts-jest": "26.1.0",
    "ts-loader": "7.0.5",
    "typescript": "3.9.5"
  }
}
