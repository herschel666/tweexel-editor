{
  "name": "tweexel-editor",
  "description": "Editor for emoji-based pixel art on twitter.",
  "version": "0.1.0-alpha.01",
  "main": "index.html",
  "keywords": [],
  "author": "Emanuel Kluge <kraemerseele@gmx.de>",
  "license": "MIT",
  "scripts": {
    "start": "preact watch",
    "build": "preact build",
    "postbuild": "cp build/404/index.html build/404.html",
    "lint": "eslint --fix src/**/*.{js,ts,tsx}",
    "test": "jest",
    "preserve": "npm run build",
    "serve": "serve -s -C -n build",
    "precommit": "lint-staged",
    "deploy": "npm-run-all lint test build"
  },
  "lint-staged": {
    "*.{css,md,scss}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintIgnore": [
    "build/*"
  ],
  "dependencies": {
    "classnames": "2.2.6",
    "clipboard-polyfill": "2.8.6",
    "preact": "10.5.5",
    "preact-render-to-string": "5.1.11",
    "preact-router": "3.2.1",
    "tailwindcss": "1.9.6"
  },
  "devDependencies": {
    "@testing-library/preact": "1.0.2",
    "@types/classnames": "2.2.11",
    "@types/jest": "25.2.3",
    "@types/webpack-env": "1.15.3",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.21.5",
    "husky": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.1",
    "lint-staged": "10.5.1",
    "npm-run-all": "4.1.5",
    "preact-cli": "3.0.3",
    "prettier": "2.1.2",
    "serve": "11.3.2",
    "ts-jest": "26.4.3",
    "ts-loader": "7.0.5",
    "typescript": "3.9.5"
  }
}
